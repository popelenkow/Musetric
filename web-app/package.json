{
	"name": "musetric",
	"description": "Musetric is a JavaScript library for building music applications",
	"version": "0.0.3",
	"license": "MIT",
	"main": "dist/index.js",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react"
	],
	"scripts": {
		"dev-renderer": "webpack --watch --progress --config ./configs/webpack.config.renderer.dev.js",
		"dev-main": "webpack --config ./configs/webpack.config.main.dev.js",
		"predev": "rimraf dist",
		"dev": "cross-env WITH_MAIN=1 yarn dev-renderer",
		"build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
		"build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
		"prebuild": "rimraf dist",
		"build": "yarn build-renderer && yarn build-main",
		"lint": "eslint \"{src,configs}/**/*.{js,ts,tsx}\""
	},
	"dependencies": {},
	"devDependencies": {
		"@nestjs/cli": "7.5.1",
		"@nestjs/common": "7.4.4",
		"@nestjs/core": "7.4.4",
		"@nestjs/platform-express": "7.4.4",
		"@nestjs/schematics": "7.1.2",
		"@nestjs/serve-static": "^2.1.3",
		"@nestjs/testing": "7.4.4",
		"@types/dom-mediacapture-record": "1.0.7",
		"@types/express": "4.17.8",
		"@types/node": "14.11.8",
		"@types/react": "16.9.51",
		"@types/react-dom": "16.9.8",
		"@types/webpack-env": "^1.15.3",
		"cpx": "1.5.0",
		"cross-env": "7.0.2",
		"css-loader": "4.3.0",
		"eslint-config-musetric": "0.0.11",
		"html-webpack-plugin": "4.5.0",
		"i18next": "19.8.2",
		"immer": "7.0.9",
		"inline-worker": "1.1.0",
		"mini-css-extract-plugin": "1.0.0",
		"musetric": "0.0.5",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-i18next": "11.7.3",
		"reflect-metadata": "0.1.13",
		"rimraf": "3.0.2",
		"rxjs": "6.6.3",
		"sass": "1.27.0",
		"sass-loader": "10.0.3",
		"start-server-webpack-plugin": "2.2.5",
		"ts-loader": "8.0.4",
		"ts-node": "9.0.0",
		"tsconfig-paths": "3.9.0",
		"typescript": "4.0.3",
		"webpack": "4.44.2",
