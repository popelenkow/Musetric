{
	"name": "musetric",
	"description": "Musetric is a JavaScript library for building music applications",
	"version": "0.0.3",
	"license": "MIT",
	"main": "dist/index.js",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react"
	],
	"scripts": {
		"preinstall": "cd ../musetric & yarn",
		"dev-renderer": "webpack --watch --progress --config ./configs/webpack.config.renderer.dev.js",
		"dev-main": "webpack --config ./configs/webpack.config.main.dev.js && node dist/main.js",
		"predev": "rimraf dist && node copy.js",
		"dev": "cross-env WITH_MAIN=1 yarn dev-renderer",
		"build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
		"build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
		"prebuild": "rimraf dist && node copy.js",
		"build": "yarn build-renderer && yarn build-main",
		"lint": "eslint \"{src,configs}/**/*.{js,ts,tsx}\""
	},
	"dependencies": {},
	"devDependencies": {
		"@types/express": "4.17.11",
		"@types/node": "14.14.22",
		"copyfiles": "2.4.1",
		"cross-env": "7.0.3",
		"css-loader": "5.0.1",
		"eslint-config-musetric": "0.0.23",
		"express": "4.17.1",
		"html-webpack-plugin": "5.0.0-beta.1",
		"mini-css-extract-plugin": "1.3.4",
		"musetric": "0.0.11-dev.662",
		"rimraf": "3.0.2",
		"sass": "1.32.5",
		"sass-loader": "10.1.1",
		"ts-loader": "8.0.14",
		"ts-node": "9.1.1",
		"tsconfig-paths": "3.9.0",
		"typescript": "4.1.3",
		"webpack": "5.16.0",
		"webpack-cli": "4.4.0",
		"webpack-merge": "5.7.3",
		"webpack-node-externals": "2.5.2"
	}
}
