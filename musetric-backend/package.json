{
	"name": "musetric-backend",
	"description": "Musetric is javascript music backend",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"types": "index.d.ts",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"musetric-backend"
	],
	"scripts": {
		"dev": "nodemon --watch src src/index.ts",
		"dev1": "node --watch --loader ts-node/esm --experimental-specifier-resolution node src",
		"build": "rimraf dist && tsc -p tsconfig.json",
		"lint": "eslint **/*.{js,ts,tsx}",
		"lint:fix": "eslint **/*.{js,ts,tsx} --fix",
		"lint:config": "rimraf .eslintrc.local.json && eslint --print-config .eslintrc.js >> .eslintrc.local.json"
	},
	"devDependencies": {
		"@fastify/cors": "8.2.1",
		"@fastify/multipart": "7.6.0",
		"@fastify/static": "6.10.1",
		"@types/node": "18.15.13",
		"eslint-config-musetric": "0.0.53",
		"fastify": "4.15.0",
		"musetric-api": "0.0.3",
		"nodemon": "2.0.22",
		"rimraf": "5.0.0",
		"selfsigned": "2.1.1",
		"ts-node": "10.9.1",
		"typescript": "5.0.4"
	}
}
