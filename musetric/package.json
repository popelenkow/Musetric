{
	"name": "musetric",
	"description": "Musetric is a javascript library for building music applications",
	"version": "0.0.45",
	"license": "MIT",
	"main": "index.js",
	"types": "index.d.ts",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"scripts": {
		"build": "node scripts/build.js && tsc",
		"build:dev": "node scripts/build.js && tsc && node scripts/copyBuildToApp.js",
		"dev": "nodemon --watch src --ext ts,tsx --exec yarn build:dev",
		"lint": "eslint **/*.{js,ts,tsx}",
		"lint:fix": "eslint **/*.{js,ts,tsx} --fix",
		"lint:config": "rimraf .eslintrc.local.json && eslint --print-config .eslintrc.js >> .eslintrc.local.json",
		"parse-locale": "i18next -c ./i18next.parser.config.js",
		"test": "jest",
		"perf": "ts-node performance/index.ts"
	},
	"dependencies": {
		"classnames": "2.5.1",
		"color": "4.2.3",
		"i18next": "23.7.16",
		"immer": "10.0.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-jss": "10.10.0"
	},
	"devDependencies": {
		"@types/benchmark": "2.1.5",
		"@types/color": "3.0.6",
		"@types/jest": "29.5.11",
		"@types/node": "20.11.0",
		"@types/react": "18.2.47",
		"@types/react-dom": "18.2.18",
		"benchmark": "2.1.4",
		"copyfiles": "2.4.1",
		"eslint-plugin-musetric": "0.0.3-beta.17",
		"fft.js": "4.0.4",
		"i18next-parser": "8.12.0",
		"jest": "29.7.0",
		"nodemon": "3.0.2",
		"rimraf": "5.0.5",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.2",
		"typescript": "5.3.3"
	}
}
