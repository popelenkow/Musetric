{
	"name": "musetric",
	"description": "Musetric is a JavaScript library for building music applications.",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"types": "index.d.ts",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"copy": "cpx src/**/*.*css dist && cpx {package.json,licence.txt,readme.md,locales/**/*.json} dist",
		"prebuild": "rimraf dist && yarn copy",
		"build": "tsc",
		"parse-locale": "i18next -c ./configs/i18next.parser.config.js",
		"lint": "eslint src/**/*.{ts,tsx}"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/dom-mediacapture-record": "1.0.7",
		"@types/react": "16.9.46",
		"@types/react-dom": "16.9.8",
		"@typescript-eslint/eslint-plugin": "3.9.1",
		"@typescript-eslint/parser": "3.9.1",
		"cpx": "1.5.0",
		"eslint": "7.7.0",
		"eslint-config-airbnb": "18.2.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-react": "7.20.6",
		"eslint-plugin-react-hooks": "4.1.0",
		"i18next": "19.7.0",
		"i18next-parser": "3.0.1",
		"immer": "7.0.7",
		"inline-worker": "1.1.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-i18next": "11.7.0",
		"rimraf": "3.0.2",
		"typescript": "3.9.7"
	}
}
