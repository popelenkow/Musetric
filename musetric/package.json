{
	"name": "musetric",
	"description": "Musetric is a JavaScript library for building music applications.",
	"version": "0.0.1",
	"license": "MIT",
	"main": "index.js",
	"types": "index.d.ts",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react"
	],
	"engines": {
		"node": ">=10.0.0"
	},
	"scripts": {
		"copy": "cpx src/**/*.*css dist && cpx {package.json,licence.txt,readme.md,locale/**/*.*} dist",
		"prebuild": "rimraf dist && yarn copy",
		"build": "tsc",
		"parse-locale": "i18next -c ./configs/i18next.parser.config.js"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/dom-mediacapture-record": "1.0.7",
		"@types/react": "16.9.46",
		"@types/react-dom": "16.9.8",
		"cpx": "1.5.0",
		"i18next": "19.6.3",
		"i18next-parser": "2.1.3",
		"immer": "7.0.7",
		"inline-worker": "1.1.0",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-i18next": "11.7.0",
		"rimraf": "3.0.2",
		"typescript": "3.9.7"
	}
}
