{
	"name": "musetric-front",
	"version": "0.0.1",
	"license": "MIT",
	"main": "./dist/main.js",
	"scripts": {
		"dev-renderer": "webpack-dev-server --progress --config ./configs/webpack.config.renderer.dev.js",
		"dev-main": "webpack --progress --config ./configs/webpack.config.main.dev.js && electron .",
		"dev": "cross-env WITH_MAIN=1 yarn dev-renderer",
		"build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
		"build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
		"build": "yarn cln && yarn build-renderer && yarn build-main && electron-builder",
		"cln": "rimraf dist && rimraf build",
		"parse-locale": "i18next -c ./configs/i18next-parser.config.js"
	},
	"author": {
		"name": "Musetric",
		"email": "musetric@musetric.musetric",
		"url": "https://github.com/popelenkow/Musetric"
	},
	"description": "Musetric app description",
	"bugs": {
		"url": "https://github.com/popelenkow/Musetric/issues"
	},
	"build": {
		"appId": "musetric.app",
		"productName": "Musetric",
		"files": [
			"./dist/**/*"
		],
		"directories": {
			"output": "build"
		},
		"win": {
			"target": [
				"nsis"
			],
			"icon": "./icon.ico"
		},
		"nsis": {
			"allowToChangeInstallationDirectory": true,
			"oneClick": false
		},
		"extraFiles": [
			"background"
		]
	},
	"dependencies": {},
	"devDependencies": {
		"@types/classnames": "2.2.10",
		"@types/react": "16.9.41",
		"@types/react-dom": "16.9.8",
		"cross-env": "7.0.2",
		"css-loader": "3.6.0",
		"electron": "9.1.0",
		"electron-builder": "22.7.0",
		"html-webpack-plugin": "4.3.0",
		"i18next": "19.5.5",
		"i18next-parser": "2.1.0",
		"immer": "7.0.5",
		"mini-css-extract-plugin": "0.9.0",
		"python-shell": "2.0.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-i18next": "11.7.0",
		"sass": "1.26.10",
		"sass-loader": "9.0.2",
		"ts-loader": "8.0.0",
		"typescript": "3.9.6",
		"wait-on": "5.0.3",
		"webpack": "4.43.0",
		"webpack-cli": "3.3.12",
		"webpack-dev-server": "3.11.0",
		"webpack-merge": "5.0.9"
	}
}
