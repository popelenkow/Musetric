{
  "name": "musetric-front",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Musetric app description",
  "author": "Musetric <musetric@musetric.musetric>",
  "main": "./dist/electron.js",
  "scripts": {
    "dev-renderer": "webpack-dev-server --progress --config ./configs/webpack.config.renderer.dev.js",
    "dev-main": "wait-on http://localhost:8080/ && webpack --progress --config ./configs/webpack.config.main.dev.js && electron .",
    "dev": "concurrently \"yarn dev-renderer\" \"yarn dev-main\" ",
    "build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
    "build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
    "build": "rimraf dist && yarn build-renderer && yarn build-main && rimraf build && electron-builder",
    "cln": "rimraf dist && rimraf build"
  },
  "build": {
    "appId": "musetric.app",
    "productName": "Musetric",
    "files": [
      "./dist/**/*"
    ],
    "directories": {
      "output": "build"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "concurrently": "^5.2.0",
    "electron": "^9.0.2",
    "electron-builder": "^22.7.0",
    "html-webpack-plugin": "^4.3.0",
    "immer": "^6.0.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "wait-on": "^5.0.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
