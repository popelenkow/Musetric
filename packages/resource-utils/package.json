{
  "name": "@musetric/resource-utils",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "monorepo": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "project:build": "tsc --project tsconfig.build.json",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint . --cache",
    "check:lint:fix": "eslint . --fix --cache",
    "test": "vitest run"
  },
  "devDependencies": {
    "@musetric/eslint-config": "workspace:*",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
