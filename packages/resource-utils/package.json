{
  "name": "@musetric/resource-utils",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "monorepo": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./cross/*": {
      "monorepo": "./src/*.cross.ts",
      "types": "./dist/*.cross.d.ts",
      "import": "./dist/*.cross.js",
      "default": "./dist/*.cross.js"
    },
    "./node": {
      "monorepo": "./src/index.node.ts",
      "types": "./dist/index.node.d.ts",
      "import": "./dist/index.node.js",
      "default": "./dist/index.node.js"
    }
  },
  "scripts": {
    "project:build": "tsc --build tsconfig.build.json",
    "check:ts": "tsc --build",
    "check:lint": "eslint . --cache",
    "check:lint:fix": "eslint . --fix --cache",
    "test": "vitest run"
  },
  "devDependencies": {
    "@musetric/eslint-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
