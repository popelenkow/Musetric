    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "16.4.0",
    "jiti": "2.6.0",
    "typescript-eslint": "8.44.1"
  },
  "devDependencies": {
    "eslint": "9.36.0",
    "typescript": "5.9.2"
  }
