{
  "name": "@musetric/eslint-config",
  "version": "0.1.0",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "scripts": {
    "project:build": "tsc --project tsconfig.build.json",
