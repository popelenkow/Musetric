{
  "name": "@musetric/frontend",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:start": "vite -- musetric-frontend",
    "dev:stop": "kill-process musetric-frontend",
    "project:build": "vite build && cpx \"dist/**/*\" ../backend/storage/public --clean",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint . --cache",
    "check:lint:fix": "eslint . --fix --cache",
    "check:translations": "i18next -c ./i18next.parser.config.ts --fail-on-update",
    "check:translations:fix": "i18next -c ./i18next.parser.config.ts"
  },
  "devDependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.1",
    "@hookform/resolvers": "5.2.1",
    "@mui/icons-material": "7.3.5",
    "@mui/material": "7.3.5",
    "@musetric/api": "workspace:*",
    "@musetric/audio-in-out": "workspace:*",
    "@musetric/audio-view": "workspace:*",
    "@musetric/eslint-config": "workspace:*",
    "@musetric/kill-process": "workspace:*",
    "@musetric/spa-router": "workspace:*",
    "@tanstack/react-query": "5.84.2",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-react": "5.0.0",
    "axios": "1.12.2",
    "cpx2": "8.0.0",
    "date-fns": "4.1.0",
    "eslint": "9.36.0",
    "framer-motion": "12.23.12",
    "globals": "16.4.0",
    "i18next": "25.3.2",
    "i18next-parser": "9.3.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.62.0",
    "react-i18next": "15.6.1",
    "typescript": "5.9.2",
    "vite": "7.1.1",
    "vite-plugin-mkcert": "1.17.8",
    "zod": "4.1.9",
    "zustand": "5.0.7"
  }
}
