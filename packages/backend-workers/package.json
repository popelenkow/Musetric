{
  "name": "@musetric/backend-workers",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "monorepo": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./scripts": {
      "monorepo": "./src/scripts/index.ts",
      "types": "./dist/scripts/index.d.ts",
      "import": "./dist/scripts/index.js",
      "default": "./dist/scripts/index.js"
    }
  },
  "scripts": {
    "postinstall": "node --conditions=monorepo scripts/install.ts",
    "project:build": "tsc --project tsconfig.build.json",
    "check:ts": "tsc --noEmit",
    "check:lint": "eslint . --cache && uv run python scripts/check.py",
    "check:lint:fix": "eslint . --fix --cache && uv run python scripts/check.py --fix"
  },
  "peerDependencies": {
    "@musetric/resource-utils": "workspace:*"
  },
  "devDependencies": {
    "@musetric/eslint-config": "workspace:*",
    "@musetric/resource-utils": "workspace:*",
    "@types/node": "24.2.1",
    "axios": "1.12.2",
    "eslint": "9.36.0",
    "typescript": "5.9.2"
  }
}
