{
	"name": "musetric-desktop-app",
	"description": "Musetric desktop application",
	"version": "0.0.1",
	"license": "MIT",
	"main": "./dist/main.js",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react",
		"electron"
	],
	"author": {
		"name": "Musetric",
		"email": "popelenkow@gmail.com",
		"url": "https://github.com/popelenkow/Musetric"
	},
	"scripts": {
		"dev-renderer": "webpack serve --progress --config ./configs/webpack.config.renderer.dev.js",
		"dev-main": "webpack --progress --config ./configs/webpack.config.main.dev.js && electron .",
		"dev": "cross-env WITH_MAIN=1 yarn dev-renderer",
		"build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
		"build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
		"prebuild": "rimraf dist && yarn build-renderer && yarn build-main",
		"build": "rimraf build && electron-builder --publish never",
		"lint": "eslint \"{src,configs}/**/*.{js,ts,tsx}\" "
	},
	"dependencies": {},
	"devDependencies": {
		"cpx": "1.5.0",
		"cross-env": "7.0.2",
		"css-loader": "5.0.0",
		"electron": "10.1.5",
		"electron-builder": "22.9.1",
		"eslint-config-musetric": "0.0.15",
		"html-webpack-plugin": "5.0.0-alpha.6",
		"mini-css-extract-plugin": "1.2.0",
		"musetric": "0.0.6",
		"python-shell": "2.0.2",
		"sass": "1.27.0",
		"sass-loader": "10.0.4",
		"ts-loader": "8.0.7",
		"typescript": "4.0.3",
		"webpack": "5.2.0",
		"webpack-cli": "4.1.0",
		"webpack-dev-server": "3.11.0",
		"webpack-merge": "5.2.0"
	}
}
