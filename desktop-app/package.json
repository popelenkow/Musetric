{
	"name": "musetric-desktop-app",
	"description": "Musetric desktop application",
	"version": "0.0.1",
	"license": "MIT",
	"main": "./dist/main.js",
	"homepage": "https://github.com/popelenkow/Musetric",
	"bugs": "https://github.com/popelenkow/Musetric/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/popelenkow/Musetric",
		"directory": "musetric"
	},
	"keywords": [
		"musetric",
		"react",
		"electron"
	],
	"author": {
		"name": "Musetric",
		"email": "musetric@musetric.musetric",
		"url": "https://github.com/popelenkow/Musetric"
	},
	"scripts": {
		"postinstall": "cpx node_modules/musetric/locales/**/*.* locale",
		"dev-renderer": "webpack-dev-server --progress --config ./configs/webpack.config.renderer.dev.js",
		"dev-main": "webpack --progress --config ./configs/webpack.config.main.dev.js && electron .",
		"dev": "cross-env WITH_MAIN=1 yarn dev-renderer",
		"build-renderer": "webpack --progress --config ./configs/webpack.config.renderer.prod.js",
		"build-main": "webpack --progress --config ./configs/webpack.config.main.prod.js",
		"prebuild": "rimraf dist && yarn build-renderer && yarn build-main",
		"build": "rimraf build && electron-builder",
		"lint": "eslint src/**/*.{ts,tsx}"
	},
	"dependencies": {
		"musetric": "0.0.1-dev.18"
	},
	"devDependencies": {
		"@types/classnames": "2.2.10",
		"@types/dom-mediacapture-record": "1.0.7",
		"@types/react": "16.9.49",
		"@types/react-dom": "16.9.8",
		"cpx": "1.5.0",
		"cross-env": "7.0.2",
		"css-loader": "4.2.2",
		"electron": "10.1.1",
		"electron-builder": "22.8.0",
		"eslint-musetric": "0.0.5",
		"html-webpack-plugin": "4.4.1",
		"i18next": "19.7.0",
		"immer": "7.0.8",
		"inline-worker": "1.1.0",
		"mini-css-extract-plugin": "0.11.0",
		"python-shell": "2.0.1",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-i18next": "11.7.2",
		"sass": "1.26.10",
		"sass-loader": "10.0.1",
		"ts-loader": "8.0.3",
		"typescript": "4.0.2",
		"webpack": "4.44.1",
		"webpack-cli": "3.3.12",
		"webpack-dev-server": "3.11.0",
		"webpack-merge": "5.1.3"
	}
}
